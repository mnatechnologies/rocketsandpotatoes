{
  "name": "australian-national-bullion",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "dotenvx run --env-file=.env.development -- next dev --turbopack",
    "build": "dotenvx run --env-file=.env.production -- next build --turbopack",
    "start": "dotenvx run --env-file=.env.production -- next start",
    "lint": "eslint",
    "test": "vitest --run",
    "test:watch": "vitest",
    "test:coverage": "vitest --coverage",
    "dry-run": "dotenvx run --env-file=.env.development -- tsx src/scripts/dry-run-checkout.ts",
    "dry-run:single": "tsx src/scripts/dry-run-checkout.ts",
    "test:checkout": "dotenvx run --env-file=.env.development vitest run src/test/checkout-flow.test.ts",
    "test:checkout:watch": "dotenvx run -- vitest watch src/test/checkout-flow.test.ts",
    "test:ttr": "dotenvx run --env-file=.env.development -- vitest run src/test/ttr-reporting.test.ts",
    "test:ttr:watch": "dotenvx run --env-file=.env.development -- vitest watch src/test/ttr-reporting.test.ts",
    "test:smr": "dotenvx run --env-file=.env.development -- vitest run src/test/smr-generation.test.ts",
    "test:smr:watch": "dotenvx run --env-file=.env.development -- vitest watch src/test/smr-generation.test.ts",
    "test:compliance": "dotenvx run --env-file=.env.development -- tsx src/test/run-compliance-tests.ts",
    "test:compliance-all": "dotenvx run --env-file=.env.development -- vitest run src/test/ttr-reporting.test.ts src/test/smr-generation.test.ts",
    "seed-test": "dotenvx run --env-file=.env.development -- tsx src/test/seed-test-data.ts",
    "import-sanctions": "npx dotenvx run --env-file=.env.development --  tsx src/scripts/import-dfat-sanctions.ts",
    "ttrsmr-report-test": "dotenvx run --env-file=.env.development -- tsx src/scripts/quick-test.ts",
    "delete-clerk-users": "dotenvx run --env-file=.env.development -- tsx src/scripts/delete-all-clerk-users.ts",
    "sync-clerk-users": "dotenvx run --env-file=.env.development  -- tsx src/scripts/sync-clerk-users.ts",
    "test-api-pricing": "dotenvx run --env-file=.env.development node src/test/test-metalpriceapi-direct.js"
  },
  "dependencies": {
    "@aws-sdk/client-ses": "^3.940.0",
    "@clerk/nextjs": "^6.33.7",
    "@icons-pack/react-simple-icons": "^13.8.0",
    "@ngrok/ngrok": "^1.5.2",
    "@react-email/components": "^1.0.1",
    "@react-email/render": "^2.0.0",
    "@stripe/react-stripe-js": "^5.2.0",
    "@stripe/stripe-js": "^8.1.0",
    "@supabase/supabase-js": "^2.75.1",
    "@vercel/functions": "^3.3.4",
    "fast-xml-parser": "^5.3.2",
    "lucide-react": "^0.545.0",
    "next": "^16.0.10",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-email": "^5.0.4",
    "sonner": "^2.0.7",
    "stripe": "^19.1.0",
    "svix": "^1.77.0",
    "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.51.1",
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^24.10.1",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9",
    "eslint-config-next": "15.5.5",
    "supabase": "^2.70.5",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^3.2.4",
    "whatwg-url": "^15.1.0"
  }
}
